    "date-fns": "^2.30.0",
    "express": "^4.17.1",
    "node-fetch": "^3.3.1",
    "nuxt": "2.16.3",
    "nuxt-client-init-module": "^0.3.0",
    "openai": "^3.3.0",
    "page-metadata-parser": "^1.1.4",
    "robots-parser": "^3.0.1",
    "ua-parser-js": "^1.0.37",
    "ufo": "^1.5.3",
