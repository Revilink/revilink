{
  "name": "revilink",
  "version": "1.3.5",
  "description": "Comment on any link on the internet",
  "private": true,
  "keywords": [
    "revilink",
    "reviews",
    "comments",
    "link review"
  ],
  "author": "selimdoyranli",
  "contributors": [
    "Selim DoyranlÄ± <selimdoyranli@gmail.com> (https://selimdoyranli.com)"
  ],
